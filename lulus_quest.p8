	if (btn(â¬‡ï¸)) ysign = 1
	if ((btn(â¬…ï¸)) or (btn(âž¡ï¸)) or (btn(â¬†ï¸)) or (btn(â¬‡ï¸))) dirpressed = true

	if dirpressed then
			local x = ima_light_bo.x + xsign * 8
			local y = ima_light_bo.y + ysign * 8
			
			-- Vã‚ŒãŸrification du dã‚ŒãŸplacement normal
